;捕虜の扱い関係

;-------------------------------------------------
;捕虜の扱いを設定する
;-------------------------------------------------
@SETSTATE_PRISONER
#DIM FIRST_LINE
;1ページ22人までのコマンドが表示される
;LOCAL	
	;0:ループ用
	;5:捕虜の人数/選んだボタンの対象キャラ
	;6:選んだボタンの捕虜の扱い
	;7:表示した行数をカウントする
	;8:ループ内で捕虜の人数をカウントするためにある
	;10:最大ページ
	;11:現在のページ
	;15:表示位置調整用
	;20:触手調教可能

;●捕虜の人数をカウント
LOCAL:5 = 0
FOR LOCAL:0, 0, CHARANUM
	IF LOCAL:0 != MASTER && CFLAG:(LOCAL:0):捕虜先 != 0 && CFLAG:(LOCAL:0):捕虜先 == CFLAG:MASTER:所属 && CFLAG:(LOCAL:0):監禁状態 == 0
		LOCAL:5 ++
	ENDIF
NEXT

;捕虜が一人もいなければ戻る
IF LOCAL:5 == 0
	RETURN
ENDIF

;●表示部分
LOCAL:10 = (LOCAL:5 - 1) / 22 + 1
LOCAL:11 = 1

;触手調教が可能かどうかを判定
LOCAL:20 = ITEM:触手部屋 && ID_TO_CHARA(FLAG:触手部屋管理者) >= 0 && IS_SYOKUSYU_MANAGE(ID_TO_CHARA(FLAG:触手部屋管理者))
FIRST_LINE = LINECOUNT
$SHOW_LOOP

DRAWLINE
PRINTFORML 各捕虜の扱いを設定して下さい
DRAWLINE

PRINTFORM %TOSTR_SPACE(47)%
PRINTFORM <一括> 
PRINTBUTTON "{拘留}",10
PRINTBUTTON "{拷問}",12
PRINTBUTTON "{Ｃ開発}",13
PRINTBUTTON "{Ｂ開発}",14
PRINTBUTTON "{性処理}",15
IF LOCAL:20
	PRINTBUTTON "{触手調教}",16
ENDIF
PRINTBUTTON "{娼館}",17
PRINTBUTTON "{搾乳}",18
PRINTL

LOCAL:7 = 0
LOCAL:8 = 0
LOCAL:12 = (LOCAL:11 - 1) * 22
LOCAL:13 = LOCAL:11 * 22
FOR LOCAL:0, 0, CHARANUM
	IF LOCAL:0 != MASTER && CFLAG:(LOCAL:0):捕虜先 == CFLAG:MASTER:所属 && CFLAG:(LOCAL:0):監禁状態 == 0
		IF LOCAL:8 >= LOCAL:12 && LOCAL:8 < LOCAL:13
			PRINTBUTTON @"{NO:(LOCAL:0)+100, 4}[%ANAME(LOCAL:0), 16, LEFT%]", NO:(LOCAL:0)+100
			;海外から逆輸入
			;Hi.
			;You may be able to use #DIM/#DIMS instead using LOCAL/LOCALS. Emuera Wiki says that LOCAL/LOCALS are obsolete.
			PRINT  
			CALL PRINT_SEX(LOCAL:0, 1)
			CALL TMP_PRINT_CHARA_STARS_NUM(LOCAL:0)
			IF TALENT:(LOCAL:0):崩壊
				CALL COLORPRINT("崩", 0xFF0000)
			ELSEIF TALENT:(LOCAL:0):虚ろ
				CALL COLORPRINT("虚", 0xEE7800)
			ELSE
				PRINT   
			ENDIF
			PRINTFORM  %@"精神:{(BASE:(LOCAL:0):精神力 * 100 / MAXBASE:(LOCAL:0):精神力)}", 9, LEFT%
			IF CFLAG:(LOCAL:0):所属
				PRINTFORM 所属:%SNAME(GET_COUNTRY_BOSS(CFLAG:LOCAL:所属)), MAX_CHARANAME_LENGTH / 2, LEFT%
			ELSE
				PRINTFORM 所属:%@"----", MAX_CHARANAME_LENGTH / 2, LEFT%
			ENDIF
			CALL SETSTATE_PRISONER_BUTTON(LOCAL:0, 0, "拘留")
			CALL SETSTATE_PRISONER_BUTTON(LOCAL:0, 2, "拷問")
			CALL SETSTATE_PRISONER_BUTTON(LOCAL:0, 3, "Ｃ開発")
			CALL SETSTATE_PRISONER_BUTTON(LOCAL:0, 4, "Ｂ開発")
			CALL SETSTATE_PRISONER_BUTTON(LOCAL:0, 5, "性処理")
			IF LOCAL:20
				CALL SETSTATE_PRISONER_BUTTON(LOCAL:0, 6, "触手調教")
			ENDIF
			CALL SETSTATE_PRISONER_BUTTON(LOCAL:0, 7, "娼館")
			CALL SETSTATE_PRISONER_BUTTON(LOCAL:0, 8, "搾乳")
			PRINTL 
			LOCAL:7 ++
		ENDIF
		LOCAL:8 ++
	ENDIF
NEXT
DRAWLINE

IF LOCAL:10 >= 2
	LOCAL:7 += 2

	LOCAL:15 = 25
	IF LOCAL:11 >= 2
		PRINTBUTTON "[8] 前のページ", 8
		LOCAL:15 -= 12
	ENDIF
	PRINTFORM %TOSTR_SPACE(LOCAL:15)%

	LOCALS:0 = page{LOCAL:11}/{LOCAL:10}
	PRINTPLAINFORM %LOCALS:0, 25, LEFT%

	IF LOCAL:11 < LOCAL:10
		PRINTBUTTON "[9] 次のページ", 9
	ENDIF
	PRINTL 
	DRAWLINE
ENDIF
PRINTBUTTON "[0] 決  定", 0

REDRAW 0

$INPUT_LOOP
INPUT

IF RESULT == 0
	REDRAW 1
	RETURN
ELSEIF RESULT == 8 && LOCAL:11 >= 2
	LOCAL:11 --
ELSEIF RESULT == 9 && LOCAL:11 < LOCAL:10
	LOCAL:11 ++
ELSEIF RESULT == 10 || (RESULT >= 12 && RESULT <= 15) || (RESULT == 16 && LOCAL:20) || RESULT == 17 || RESULT == 18
	CALL SETSTATE_PRISONER_SETALL(RESULT - 10)
ELSEIF RESULT >= 100 && RESULT <= 999
	CALL USERSHOP_LIFE_CHARA1(NO_TO_CHARA(RESULT - 100))
ELSEIF RESULT >= 1000
	LOCAL:5 = RESULT / 1000 - 1
	LOCAL:6 = RESULT % 1000
	IF LOCAL:5 < CHARANUM && LOCAL:5 != MASTER && CFLAG:(LOCAL:5):捕虜先 == CFLAG:MASTER:所属 && CFLAG:(LOCAL:5):監禁状態 == 0
		IF CFLAG:(LOCAL:5):捕虜の扱い != LOCAL:6 && (LOCAL:6 == 0 || (LOCAL:6 >= 2 && LOCAL:6 <= 5) || (LOCAL:6 == 6 && LOCAL:20)) || LOCAL:6 == 7 || LOCAL:6 == 8
			CFLAG:(LOCAL:5):捕虜の扱い = LOCAL:6
		ENDIF
	ENDIF
ELSE
	CLEARLINE 1
	GOTO INPUT_LOOP
ENDIF
CLEARLINE LINECOUNT - FIRST_LINE
GOTO SHOW_LOOP

