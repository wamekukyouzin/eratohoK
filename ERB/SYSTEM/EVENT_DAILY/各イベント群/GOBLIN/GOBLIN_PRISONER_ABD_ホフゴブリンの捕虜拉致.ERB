;---------------------
;特定の条件を満たすキャラをランダムに選択する場合に利用
;他の関数は必須だが、これだけはなくてもよい　というかパフォーマンスへ影響するので不要なら作ってはならない
;対象が存在せずデイリーを開始できない場合は0を返すことでデイリーの発生をキャンセルする
;---------------------
@EVENT_DAILY_GOBLIN_PRISONER_ABD_SETTARGET()
#DIM ゴブリン
ゴブリン = GET_COUNTRY_FROM_ID(SP_COUNTRY_ID:(特殊勢力_ホフゴブリン))

FOR LOCAL, 0, CHARANUM
	;ゴブリン勢力に所属しておらず、ゴブリン勢力の捕虜でもなく、君主でもなく、妊娠している動物以外の女性キャラから選択
	IF CFLAG:(LOCAL):所属 != ゴブリン && !GROUPMATCH(CFLAG:(LOCAL):捕虜先, 0, ゴブリン) && IS_FEMALE(LOCAL) && !IS_ANIMAL(LOCAL) && !IS_SP_CHARA(LOCAL) && !TALENT:LOCAL:特殊勢力素質 && !IS_SP_COUNTRY(CFLAG:LOCAL:捕虜先)
		DAILY_TARGET:DAILY_TARGET_NUM = LOCAL
		DAILY_TARGET_NUM ++
	ENDIF
NEXT
SIF DAILY_TARGET_NUM < 1
	RETURN 0
RETURN 1


