@CHECK_STRBODY(ARGS, ARG:0)
#FUNCTION
#DIM LCOUNT
#DIM 分割数

VARSET 分割数
VARSET LOCAL
VARSET LOCALS

SPLIT ARGS, "：", LOCALS
分割数 = RESULT
LOCAL:0 = 1
FOR LCOUNT, 0, 分割数
	LOCALS:11 = %LOCALS:LCOUNT%
