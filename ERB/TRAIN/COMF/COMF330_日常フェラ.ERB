;-------------------------------------------------
@COM_ABLE330
;共通部分
CALL COM_ABLE_COMMON(330)
SIF RESULT == 0
	RETURN 0
;ターゲットに竿が必要
SIF !HAS_PENIS(MTAR:0)
	RETURN 0
;行動不能なら不可
SIF !IS_PLAYABLE(MPLY:0)
	RETURN 0
;キス中は不可
SIF IS_EQUIP(MPLY:0, 342)
	RETURN 0
;主人公以外が実行する場合、実行可能でなければ表示されない
