;-----------------------------------
;基本値計算に先んじて処理するもの
;-----------------------------------
@SKILL_89_SP_0_EXIST
RETURN 1

@SKILL_89_SP_0_NAME
RESULTS = レベルティターニア

;レベルは1-5まで
@SKILL_89_SP_0_LEVEL
RETURN 5

;対象選択
@SKILL_89_SP_0_SETTARGET(発動者, 発動番号, 発動側, 発動勢力, 発動部隊, 対象勢力, 対象部隊)
#DIM 発動者
#DIM 発動番号
#DIM スキル
#DIMS ジャンル
#DIM 発動側
#DIM 発動勢力
#DIM 発動部隊
#DIM 対象側
#DIM 対象勢力
#DIM 対象部隊
#DIM 能力, 3
VARSET 能力
COMBAT_SKILL_TARGET = 発動番号
RETURN 1

;発動判定
@SKILL_89_SP_0_CAN_INVOKE(発動者, 発動番号, 発動側, 発動勢力, 発動部隊, 対象勢力, 対象部隊)
#DIM 発動者
#DIM 発動番号
#DIM スキル
#DIMS ジャンル
#DIM 発動側
#DIM 発動勢力
#DIM 発動部隊
#DIM 対象側
#DIM 対象勢力
#DIM 対象部隊
対象側 = !発動側
FOR LOCAL, 0, BATTLE_COMMANDER_NUM:発動側
	IF BATTLE_COMMANDER:発動側:LOCAL == NAME_TO_CHARA("アリス")
		RETURN 1
	ENDIF
NEXT
RETURN 0

;効果をここに記述
@SKILL_89_SP_0_INVOKE(発動者, 発動番号, 発動側, 発動勢力, 発動部隊, 対象勢力, 対象部隊)
#DIM 発動者
#DIM 発動番号
#DIM スキル
#DIMS ジャンル
#DIM 発動側
#DIM 発動勢力
#DIM 発動部隊
#DIM 対象側
#DIM 対象勢力
#DIM 対象部隊
対象側 = !発動側
PRINTFORML %ANAME(発動者)%の武闘が増加した！
TIMES BATTLE_武闘パワー:発動側:COMBAT_SKILL_TARGET, 1.5
RETURN 1

;
@SKILL_89_SP_0_EXPLANATION
RESULTS = 自部隊にアリスがいるとき発動。自身の武闘を上昇させる。

@SKILL_89_SP_0_CANT_TELL


@SKILL_89_SP_0_RATE_SHOW
RESULTS = 1000
RETURN 0

@SKILL_89_SP_0_RATE_ACTUAL(発動者, 発動番号, 発動側, 発動勢力, 発動部隊, 対象勢力, 対象部隊)
#DIM 発動者
#DIM 発動番号
#DIM スキル
#DIMS ジャンル
#DIM 発動側
#DIM 発動勢力
#DIM 発動部隊
#DIM 対象側
